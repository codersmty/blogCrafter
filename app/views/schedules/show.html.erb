<p>
  <strong>Titile:</strong>
  <%= @schedule.titile %>
</p>

<p>
  <strong>Content:</strong>
  <%= @schedule.content %>
</p>

<p>
  <strong>Published:</strong>
  <%= @schedule.published %>
</p>

<% @schedule.comments.each do |p| %>
  <tr>
    <td><%= p.comment%></td><br>
  </tr>
<%end%>
<%= render partial: 'comments/form', locals:{comment: @comment, schedule: @schedule}%>

<% if user_signed_in? %>
<%= link_to 'Edit', edit_schedule_path(@schedule) %> |
<% end %>
<%= link_to 'Back', schedules_path %>
